{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nexport default {\n  props: {\n    ingredients: Array\n  },\n  data() {\n    return {\n      unitPrices: {\n        \"절임배추\": 600,\n        \"무\": 500,\n        \"마늘\": 7000,\n        \"생강\": 8000,\n        \"고춧가루\": 15000,\n        \"대파\": 3000,\n        \"양파\": 2500,\n        \"쪽파\": 3500,\n        \"갓\": 4500,\n        \"부추\": 4000,\n        \"다시육수\": 5000,\n        \"찹쌀풀\": 3000,\n        \"멸치액젓\": 7000,\n        \"새우젓\": 9000,\n        \"황석어젓\": 11000,\n        \"갈치젓\": 12000,\n        \"정백당\": 2000,\n        \"미원\": 3000\n      },\n      showDetails: false\n    };\n  },\n  computed: {\n    costData() {\n      return this.ingredients.map(ing => ({\n        name: ing.type,\n        percentage: ing.percentage,\n        unitPrice: this.unitPrices[ing.type] || 0\n      }));\n    },\n    totalCost() {\n      return this.costData.reduce((sum, item) => sum + item.percentage / 100 * item.unitPrice, 0);\n    }\n  }\n};","map":{"version":3,"names":["props","ingredients","Array","data","unitPrices","showDetails","computed","costData","map","ing","name","type","percentage","unitPrice","totalCost","reduce","sum","item"],"sources":["C:\\Users\\yth98\\Documents\\KimchiLabX\\src\\components\\CostOutput.vue"],"sourcesContent":["<template>\r\n  <div class=\"cost-output\">\r\n    <h4>예상 단가: {{ totalCost.toFixed(2) }} 원 \r\n      <button class=\"detail-btn\" @click=\"showDetails = true\">자세히 ></button>\r\n    </h4>\r\n\r\n    <!-- 모달 창 -->\r\n    <div v-if=\"showDetails\" class=\"modal-overlay\" @click=\"showDetails = false\">\r\n      <div class=\"modal-content\" @click.stop>\r\n        <h3>상세 단가 정보</h3>\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>재료</th>\r\n              <th>배합비(%)</th>\r\n              <th>원가(원/kg)</th>\r\n              <th>최종 원가(원)</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(item, index) in costData\" :key=\"index\">\r\n              <td>{{ item.name }}</td>\r\n              <td>{{ item.percentage }}%</td>\r\n              <td>{{ item.unitPrice.toLocaleString() }} 원/kg</td>\r\n              <td>{{ (item.percentage / 100 * item.unitPrice).toFixed(2).toLocaleString() }} 원</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <button class=\"close-btn\" @click=\"showDetails = false\">닫기</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    ingredients: Array\r\n  },\r\n  data() {\r\n    return {\r\n      unitPrices: {\r\n        \"절임배추\": 600, \"무\": 500, \"마늘\": 7000, \"생강\": 8000, \"고춧가루\": 15000,\r\n        \"대파\": 3000, \"양파\": 2500, \"쪽파\": 3500, \"갓\": 4500, \"부추\": 4000,\r\n        \"다시육수\": 5000, \"찹쌀풀\": 3000, \"멸치액젓\": 7000, \"새우젓\": 9000,\r\n        \"황석어젓\": 11000, \"갈치젓\": 12000, \"정백당\": 2000, \"미원\": 3000\r\n      },\r\n      showDetails: false\r\n    };\r\n  },\r\n  computed: {\r\n    costData() {\r\n      return this.ingredients.map(ing => ({\r\n        name: ing.type,\r\n        percentage: ing.percentage,\r\n        unitPrice: this.unitPrices[ing.type] || 0\r\n      }));\r\n    },\r\n    totalCost() {\r\n      return this.costData.reduce((sum, item) => sum + (item.percentage / 100 * item.unitPrice), 0);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.cost-output {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  margin: 5px;\r\n  position: relative;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n}\r\n\r\nh4 {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n}\r\n\r\n/* 자세히 버튼 스타일 */\r\n.detail-btn {\r\n  padding: 5px 12px;\r\n  background: #007BFF;\r\n  color: white;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n  border-radius: 5px;\r\n  transition: background 0.2s;\r\n}\r\n.detail-btn:hover {\r\n  background: #0056b3;\r\n}\r\n\r\n/* 모달 스타일 */\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 1000;\r\n}\r\n.modal-content {\r\n  background: white;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  width: 60%;\r\n  max-width: 500px;\r\n  text-align: center;\r\n  box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.3);\r\n}\r\n.close-btn {\r\n  margin-top: 10px;\r\n  padding: 5px 10px;\r\n  background: #dc3545;\r\n  color: white;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n  border-radius: 5px;\r\n}\r\n.close-btn:hover {\r\n  background: #b02a37;\r\n}\r\n\r\n/* 테이블 스타일 */\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 10px;\r\n}\r\nth, td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: center;\r\n}\r\nth {\r\n  background: #f4f4f4;\r\n}\r\n</style>\r\n"],"mappings":";;;AAmCA,eAAe;EACbA,KAAK,EAAE;IACLC,WAAW,EAAEC;EACf,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;QACV,MAAM,EAAE,GAAG;QAAE,GAAG,EAAE,GAAG;QAAE,IAAI,EAAE,IAAI;QAAE,IAAI,EAAE,IAAI;QAAE,MAAM,EAAE,KAAK;QAC5D,IAAI,EAAE,IAAI;QAAE,IAAI,EAAE,IAAI;QAAE,IAAI,EAAE,IAAI;QAAE,GAAG,EAAE,IAAI;QAAE,IAAI,EAAE,IAAI;QACzD,MAAM,EAAE,IAAI;QAAE,KAAK,EAAE,IAAI;QAAE,MAAM,EAAE,IAAI;QAAE,KAAK,EAAE,IAAI;QACpD,MAAM,EAAE,KAAK;QAAE,KAAK,EAAE,KAAK;QAAE,KAAK,EAAE,IAAI;QAAE,IAAI,EAAE;MAClD,CAAC;MACDC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACN,WAAW,CAACO,GAAG,CAACC,GAAE,KAAM;QAClCC,IAAI,EAAED,GAAG,CAACE,IAAI;QACdC,UAAU,EAAEH,GAAG,CAACG,UAAU;QAC1BC,SAAS,EAAE,IAAI,CAACT,UAAU,CAACK,GAAG,CAACE,IAAI,KAAK;MAC1C,CAAC,CAAC,CAAC;IACL,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACP,QAAQ,CAACQ,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE